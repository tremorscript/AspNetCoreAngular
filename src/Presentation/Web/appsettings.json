{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.SQLite"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.Hosting.Lifetime": "Information",
        "System": "Error"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName",
      "WithThreadId"
    ],
    "WriteTo": [
      {
        "Name": "SQLite",
        "Args": {
          "sqliteDbPath": "./logs/log.db",
          "tableName": "Logs",
          "storeTimestampInUtc": "true"
        }
      }
    ]
  },
  "Data": {
    "useInMemory": "false",
    "useSqLite": "true",
    "Web": "Data Source=Web.db"
  },
  "Auth": {
    "Authority": "https://localhost:50007/",
    "Audiences": [
      "aspnetcoreangular"
    ]
  },
  "CorsOrigins": []
}